<% if controller_name != 'sessions' %>
  <%= link_to t('.sign_in', default: 'Sign in'), new_user_session_path %><br />
<% end %>

<% if devise_mapping.recoverable? && controller_name != 'passwords' %>
  <%= link_to t('.forgot_your_password', default: 'Forgot your password?'), new_user_password_path %><br />
<% end %>